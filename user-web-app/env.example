# Social Commerce User Web Application Environment Variables

# API Configuration
REACT_APP_API_URL=https://api-dev.exalt-ecosystem.com
REACT_APP_API_VERSION=v1

# Authentication
REACT_APP_JWT_SECRET_KEY=your-jwt-secret-key
REACT_APP_AUTH_DOMAIN=auth.exalt-ecosystem.com

# Environment
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=1.0.0

# Features
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# External Services
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_key
REACT_APP_GOOGLE_ANALYTICS_ID=GA_your_tracking_id
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id

# CDN and Assets
REACT_APP_CDN_URL=https://cdn.exalt-ecosystem.com
REACT_APP_IMAGES_URL=https://images.exalt-ecosystem.com

# WebSocket
REACT_APP_WEBSOCKET_URL=wss://ws-dev.exalt-ecosystem.com

# Debug
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info