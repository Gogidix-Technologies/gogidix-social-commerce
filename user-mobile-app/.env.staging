# Staging environment variables for user-mobile-app
NODE_ENV=staging

# API Endpoints
API_BASE_URL=https://api-gateway.onrender.com
AUTH_SERVICE_URL=https://auth-service.onrender.com
MARKETPLACE_URL=https://marketplace.onrender.com
PRODUCT_SERVICE_URL=https://product-service.onrender.com
ORDER_SERVICE_URL=https://order-service.onrender.com
PAYMENT_GATEWAY_URL=https://payment-gateway.onrender.com
ANALYTICS_SERVICE_URL=https://analytics-service.onrender.com

# Service Discovery
EUREKA_CLIENT_SERVICE_URL=http://service-registry.onrender.com/eureka
CONFIG_SERVER_URL=http://config-server.onrender.com

# App Configuration
REACT_APP_API_BASE_URL=https://api-gateway.onrender.com
REACT_APP_AUTH_SERVICE_URL=https://auth-service.onrender.com
REACT_APP_MARKETPLACE_URL=https://marketplace.onrender.com
REACT_APP_PRODUCT_SERVICE_URL=https://product-service.onrender.com
REACT_APP_ORDER_SERVICE_URL=https://order-service.onrender.com
REACT_APP_PAYMENT_GATEWAY_URL=https://payment-gateway.onrender.com
REACT_APP_ANALYTICS_SERVICE_URL=https://analytics-service.onrender.com

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_SOCIAL_FEATURES=true
REACT_APP_ENABLE_MULTI_CURRENCY=true

# Deployment
VERCEL_ENV=staging
PORT=3000
