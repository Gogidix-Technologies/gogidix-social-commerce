# Exalt Social Commerce - Development Environment
# This file can be committed to git for shared development settings

# API Configuration
REACT_APP_API_BASE_URL=https://api-dev.exalt.com
REACT_APP_WEBSOCKET_URL=wss://api-dev.exalt.com/ws

# Application Configuration
REACT_APP_ENVIRONMENT=development
REACT_APP_DOMAIN=social-commerce
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_MOCK_DATA=true
REACT_APP_ENABLE_DEBUG_MODE=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=true

# Domain URLs for Development
REACT_APP_CENTRALIZED_DASHBOARD_URL=https://executive-dev.vercel.app
REACT_APP_WAREHOUSING_URL=https://warehousing-dev.vercel.app
REACT_APP_COURIER_URL=https://courier-dev.vercel.app
REACT_APP_HAULAGE_URL=https://haulage-dev.vercel.app
REACT_APP_INFRASTRUCTURE_URL=https://infrastructure-dev.vercel.app

# Custom Domain URLs (when configured)
REACT_APP_SOCIAL_COMMERCE_DOMAIN=https://social.exalt-dev.com
REACT_APP_CENTRALIZED_DASHBOARD_DOMAIN=https://executive.exalt-dev.com
REACT_APP_WAREHOUSING_DOMAIN=https://warehousing.exalt-dev.com

# Social Commerce Specific
REACT_APP_SOCIAL_MEDIA_INTEGRATION=true
REACT_APP_VENDOR_ONBOARDING_ENABLED=true
REACT_APP_PAYMENT_SANDBOX_MODE=true

# Authentication
REACT_APP_AUTH_PROVIDER=firebase
REACT_APP_OAUTH_REDIRECT_URI=https://social.exalt-dev.com/auth/callback

# Payment Configuration (Development)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_development
REACT_APP_PAYPAL_CLIENT_ID=dev_paypal_client_id

# Security Headers
REACT_APP_CSP_REPORT_URI=https://api-dev.exalt.com/security/csp-report